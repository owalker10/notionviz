{
  "name": "notionviz",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "dotenv": "^8.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development react-scripts start",
    "build": "npm run build-frontend && npm run build-functions",
    "build-frontend": "react-scripts build",
    "build-functions": "cd functions && tsc",
    "serve": "cross-env NODE_ENV=development firebase serve --only functions --port 5001",
    "shell": "firebase functions:shell",
    "start:shell": "npm run shell",
    "start:server": "npm run serve",
    "deploy": "firebase deploy --only functions,hosting",
    "build-and-deploy": "npm run build && npm run deploy",
    "logs": "firebase functions:log",
    "lint": "eslint .",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "unhost": "firebase hosting:disble",
    "del-func": "firebase functions:delete app",
    "panic": "npm run unhost && npm run del-func"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.1"
  },
  "proxy": "http://localhost:5001/notion-viz/us-central1/app"
}
